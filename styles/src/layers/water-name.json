[
  {
    "id": "waterway-name",
    "type": "symbol",
    "source": "%source%",
    "source-layer": "waterway",
    "minzoom": 12,
    "filter": [
      "all",
      [
        "==",
        "$type",
        "LineString"
      ],
      [
        "has",
        "name"
      ]
    ],
    "layout": {
      "text-font": [
        "Noto Sans Italic"
      ],
      "text-size": 14,
      "text-field": "{%name%}",
      "text-max-width": 5,
      "symbol-placement": "line",
      "text-letter-spacing": 0.2,
      "symbol-spacing": 350
    },
    "paint": {
      "text-color": "%water_text%",
      "text-halo-width": 1.5,
      "text-halo-color": "%water_halo%"
    }
  },
